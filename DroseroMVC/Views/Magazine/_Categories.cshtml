@model DroseroMVC.ViewModels.CategoryViewModel

<div>
    <ul class="zeroPadding">
    @foreach (var category in Model.Categories)
    {
        <li class="displayInline">
            <input type="button" 
                   onClick="magazineUI.onCategoryClick(@category.Id)"
                   id="@category.Id" value="@category.Name"
                   class="categoryButton"/> 
        </li>
    }
    </ul>
</div>

