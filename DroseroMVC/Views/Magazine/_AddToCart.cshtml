@model DroseroMVC.ViewModels.FoodItemViewModel
<div>
    <table data-bind="with: CartItemsViewModel">
        <thead>
        <tr>
            <td class="col-lg-1">Cart</td>
            <td class="col-lg-2">(Rs)</td>
        </tr>
        </thead>
        <tbody data-bind="foreach:FoodItems">
        <tr >
            <td class="col-lg-2"><span data-bind="text: Name"></span></td>
            <td class="col-lg-2"><span data-bind="text: Price"></span></td>
        </tr>
        </tbody>
    </table>
    <hr/>
     <div class="row">
         <span>Total: Rs</span>
         <span data-bind="text: totalValue"></span>
     </div>
</div>